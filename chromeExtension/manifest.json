{
    "manifest_version": 3,
    "name": "My Leads",
    "version": "1.1",
    "description": "An extension to save your leads",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "downloads"
    ],
    "host_permissions": [
        "blob:*"
    ]
}
